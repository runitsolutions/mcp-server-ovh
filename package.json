{
  "name": "mcp-server-ovh",
  "version": "1.0.2",
  "description": "MCP server for OVH API integration with standardized tool access",
  "main": "dist/index.js",
  "bin": {
    "mcp-server-ovh": "bin/mcp-server-ovh"
  },
  "files": [
    "bin",
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:endpoints": "node tests/integration/endpoints.test.js",
    "test:full": "node tests/e2e/full-integration.test.js",
    "test:client": "node tests/integration/client.test.js",
    "test:server": "node tests/unit/server.test.js",
    "prepublishOnly": "npm run clean && npm run build && npm run lint && npm run test"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@ovhcloud/node-ovh": "^3.0.0",
    "dotenv": "^16.4.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "typescript": "^5.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "babel-jest": "^29.7.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "mcp",
    "ovh",
    "api",
    "server",
    "model-context-protocol"
  ],
  "author": "Isaac Campos Mes√©n",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/runitsolutions/mcp-server-ovh.git"
  },
  "bugs": {
    "url": "https://github.com/runitsolutions/mcp-server-ovh/issues"
  },
  "homepage": "https://github.com/runitsolutions/mcp-server-ovh#readme"
}
